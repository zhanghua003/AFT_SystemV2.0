<Window x:Class="AFT_System.CustomControl.CustomWin.WinSetting"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AFT_System.CustomControl.CustomWin"
        mc:Ignorable="d" WindowStyle="None" WindowStartupLocation="CenterScreen" Topmost="False"
        Title="WinSetting" Height="500" Width="600">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="auto"></RowDefinition>
        </Grid.RowDefinitions>
        <Button x:Name="btn_cancel" Grid.Row="1" Height="30" Width="80" FontSize="15" Content="取消" HorizontalAlignment="Left" Margin="200,0,0,30" Click="btn_cancel_Click"></Button>
        <Button x:Name="btn_save" Grid.Row="1" Height="30" Width="80" FontSize="15" Content="保存" HorizontalAlignment="Right" Margin="0,0,200,30" Click="btn_save_Click"></Button>
    </Grid>
</Window>
